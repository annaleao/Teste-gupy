using System;

namespace SequenciaDeFibonacci
{
    class Program
    {
        static void Main(string[] args)
        {
            int n, x = 1, y = 0, z = 0;

            Console.Write("Digite um n√∫mero inteiro positivo: ");
            n = int.Parse(Console.ReadLine());

            for (int i = 0; i < n; i++)
            {
                z = x + y;
                Console.WriteLine(z);
                y = x;
                x = z;
            }

            Console.ReadLine();
        }
    }
}
